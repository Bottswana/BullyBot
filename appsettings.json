{
    "BullyBot":
    {
        "DiscordConfig":
        {
            "BotToken": ""
        },
        "Serilog":
        {
            "MinimumLevel":
            {
                "Default": "Information",
                "Override":
                {
                    "Microsoft": "Information",
                    "System": "Information"
                }
            },
            "WriteTo":
            [
                {
                    "Name": "File",
                    "Args":
                    {
                        "path": "BullyBot.log",
                        "retainedFileCountLimit": 30,
                        "rollingInterval": "Day"
                    }
                },
                {
                    "Name": "Console",
                    "Args":
                    {
                        "restrictedToMinimumLevel": "Error"
                    }
                }
            ]
        }
    }
}